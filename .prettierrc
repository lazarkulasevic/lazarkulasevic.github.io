  "printWidth": 120,
  "useTabs": false,
  "tabWidth": 4,
  "trailingComma": "none",
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
